<p id="notice"><%= notice %></p>

<h1>Phones</h1>

<table>
  <thead>
    <tr>
      <th>Phone</th>
      <th>Phone area code</th>
      <th>Phone telephone exchange</th>
      <th>Phone assigned number</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phones.each do |phone| %>
      <tr>
        <td><%= phone.phone_id %></td>
        <td><%= phone.phone_area_code %></td>
        <td><%= phone.phone_telephone_exchange %></td>
        <td><%= phone.phone_assigned_number %></td>
        <td><%= phone.person_id %></td>
        <td><%= link_to 'Show', phone %></td>
        <td><%= link_to 'Edit', edit_phone_path(phone) %></td>
        <td><%= link_to 'Destroy', phone, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

